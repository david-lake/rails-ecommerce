<% provide(:title, "Edit Profile") %>

<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <h1>Update profile</h1>
    <div class="card">
      <%= form_for(@user) do |f| %>
        <%= render 'fields', f: f %>
        <%= f.submit "Save changes", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>  
</div>
