<% provide(:title, 'New Product') %>

<h1>New Product</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4 card">
    <%= form_for(@product, html: { multipart: true }) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Add product", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
